                "USE master\n",
                "GO\n",
                "\n",
                "-- Drop the database if it already exists\n",
                "IF  EXISTS (\n",
                "        SELECT name\n",
                "        FROM sys.databases\n",
                "        WHERE name = N'saasdb'\n",
                "   )\n",
                "DROP DATABASE saasdb\n",
                "GO\n",
                "\n",
                "-- Create the database\n",
                "CREATE DATABASE saasdb\n",
                "GO"
            ],
